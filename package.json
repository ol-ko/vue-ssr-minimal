{
  "name": "vue-ssr",
  "version": "1.0.0",
  "description": "Minimal setup for SSR with Vue.js & serverless",
  "main": "server.js",
  "scripts": {
    "start": "npm run clean && npm run build && npm run serve",
    "serve": "node server",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --config ./build/webpack.client.config.js",
    "build:server": "webpack --config ./build/webpack.server.config.js",
    "clean": "rimraf dist",
    "deploy": "npm run clean && npm run build && ./node_modules/.bin/sls deploy"
  },
  "author": "Olga Skurativska",
  "license": "ISC",
  "devDependencies": {
    "@types/vue-router": "^2.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "node-sass": "^4.9.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "serverless": "^1.30.3",
    "serverless-apigw-binary": "^0.4.4",
    "ts-loader": "^4.4.2",
    "typescript": "^3.0.1",
    "vue-loader": "^15.3.0",
    "vue-property-decorator": "^7.0.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.17",
    "vuex-router-sync": "^5.0.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "aws-serverless-express": "^3.3.5",
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "sweet-modal-vue": "^2.0.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.17",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.1"
  }
}
